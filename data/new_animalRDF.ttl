@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://animal.com/Animal> a rdfs:Class .

<http://animal.com/Person> a rdfs:Class .

<captiveAnimal> rdfs:subClassOf <notCattle> .

<http://animal.com/Cattle> rdfs:subClassOf <http://animal.com/Animal> .

<http://animal.com/Chicken> a <http://animal.com/Animal> ;
    rdfs:label "Mary" ;
    foaf:age "9" .

<http://animal.com/Daphne> a <http://animal.com/Farmer> ;
    rdfs:label "Daphne the farmer" .

<http://animal.com/Dog> a <http://animal.com/Animal> ;
    rdfs:label "Keffer" ;
    foaf:age "3" .

<http://animal.com/Penguin> a <http://animal.com/Animal> ;
    rdfs:label "Ans" ;
    foaf:age "10" .

<http://animal.com/Pig> a <http://animal.com/Animal> ;
    rdfs:label "Billy" ;
    foaf:age "3" .

<http://animal.com/Zebra> a <http://animal.com/Animal> ;
    rdfs:label "Jozef" ;
    foaf:age "15" .

<http://animal.com/feedsFish> rdfs:subPropertyOf <http://animal.com/feeds> .

<http://animal.com/feedsMeat> rdfs:subPropertyOf <http://animal.com/feeds> .

<wildAnimal> rdfs:subClassOf <notCattle> .

<http://animal.com/Farmer> rdfs:subClassOf <http://animal.com/Person> .

<http://animal.com/feeds> a rdf:Property .

<notCattle> rdfs:subClassOf <Animal> .

